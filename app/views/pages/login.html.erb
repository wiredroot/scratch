	<section id="login_main">
		<div class="container">
		    <div class="row">
    		    <div id="formlogin" class="well col-sm-4 col-sm-offset-4">
                <h2>Login Form</h2>
		           <%= form_tag 'login' do  %>

                  <div class="form-group">
                      <%= label_tag :email, 'E-Mail Login:' %>
                      <%= text_field_tag :email %>
                  </div>
                  <div class="form-group">
                      <%= label_tag :password, 'Password:'  %>
                      <%= password_field_tag :password %>
                  </div>

                  <%= submit_tag 'Login', { :class => 'btn-success' } %>
                  <% end %>
                </div>


